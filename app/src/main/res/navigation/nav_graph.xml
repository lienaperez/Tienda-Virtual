<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/main_fragment">


    <fragment
        android:id="@+id/main_fragment"
        android:name="com.example.tiendavirtual.views.MainFragment"
        android:label="@string/app_name"
        tools:layout="@layout/main_fragment">
        <action
            android:id="@+id/action_main_fragment_to_other_fragment"
            app:destination="@id/other_fragment" />
    </fragment>
    <fragment
        android:id="@+id/other_fragment"
        android:name="com.example.tiendavirtual.views.MainFragment"
        android:label="@string/app_name"
        tools:layout="@layout/other_fragment"/>

</navigation>